
public class contarTama√±oNumero {

	public static void main(String[] args) {

		int numero = 56766666;
		int contador = 1;
		int[] n = new int[100];
		int aux;

		for (int i = 0; i < n.length; i++) {

			numero = numero / 10;
			if (numero > 0) {
				contador++;
			}

		}

		System.out.println(contador);
	}

}
