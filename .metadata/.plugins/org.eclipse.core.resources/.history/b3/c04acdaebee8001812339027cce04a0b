//Escribe un programa que pida 10 números por teclado y que luego muestre 
//los números introducidos junto con las palabras “máximo” y “mínimo” al lado 
//del máximo y del mínimo respectivamente.

package prueba;

import java.util.Scanner;

public class ejercicio5Tema7 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner s = new Scanner(System.in);
		
		int num[] = new int[10];
		int minimo=0;
		int maximo=0;
		
		System.out.println("Introduce 10 numeros por teclados y te diré cual es su máximo y mínimo");
		
		for(int i=0; i<10; i++) {
			
			num[i] = s.nextInt();
			
			if(num[0] < num[1]) {
				
				minimo= num[1];
				
			}
		
		System.out.println("el mínimo es "+ minimo);
		
		
		}
		
		
	}

}
