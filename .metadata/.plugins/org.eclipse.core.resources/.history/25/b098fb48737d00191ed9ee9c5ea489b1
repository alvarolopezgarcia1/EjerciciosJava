import java.util.*;
public class ejercicioBilletes {

	public static void main(String[] args) {
		
		

		Scanner s = new Scanner(System.in);
	
		System.out.println("Introduzca una cantidad");
		
		double cantidad=s.nextDouble();
		
		int contador500 = 0;
		int contador200;
		

		double [] x = {500,200,100,50,20,10,5,2,1,0.5,0.2,0.1,0.05,0.02,0.01};
		
		
			if(cantidad>500) {
				
			 contador500= (int) (cantidad/500);
				System.out.println(contador500 + " de 500€");
				
			}
			if(cantidad-(cantidad-contador500)>200) {
				
				contador200 = (int) (cantidad / 200);
				System.out.println(contador200 + " de 200€");

			}

		
		


	}

}
