
public class contarTama√±oNumero {

	public static void main(String[] args) {

		int numero = 560766666;
		int contador = 1;
		int[] n = new int[100];
		

		for (int i = 0; i < n.length; i++) {

			numero = numero / 10;
			if (numero > 0) {
				contador++;
			}

		}

		System.out.println(contador);
		
		
	}

}
