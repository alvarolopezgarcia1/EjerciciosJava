
public class contar {

	public static void main(String[] args) {

		int numero = 1425;

		int contador = 0;

		int[] copia = new int[10];


		for (int i = 0; i < numero; i++) {

			if (numero >= 0) {
				copia[i] = numero % 10;
				
			}
			numero = numero / 10;

			System.out.print(copia[i] + " ");
		}

	}
}