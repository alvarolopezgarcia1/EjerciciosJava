
public class contar {

	public static void main(String[] args) {

		int numero = 560766666;
		int contador = 1;

		for (int i = 0; i < numero; i++) {

			numero = numero / 10;
			if (numero > 0) {
				contador++;
			}

		}

		System.out.println("El numero tiene " + contador + " dígitos");

		int[] copia = new int[10];

		boolean repetido = false;

		for (int i = 0; i < numero; i++) {
			
			int aux = numero % 10;
			
			if (copia[i]==aux) {
		    repetido=true;
			contador++;

			}
			
			System.out.println("El numero " + aux + " está " + contador + " veces");
			

		}

	}
}